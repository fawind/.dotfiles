#keystate_numlock = enable
#keystate_capslock = enable
#keystate_scrolllock= enable

"dmenu_run -b"
  Print

"urxvt"
	Mod4 + Return

"urxvt -e ranger"
	Mod4 + 1

"google-chrome-stable"
	Mod4 + 2

"subl3"
	Mod4 + 3

"pcmanfm"
	Mod4 + 4

# Volume
"amixer set Master 5+"
   XF86AudioRaiseVolume

"amixer set Master 5-"
   XF86AudioLowerVolume

"amixer set Master toggle"
	XF86AudioMute

"xfce4-screenshooter"
	Mod4 + Shift + Print

"slock"
	Mod4 + Delete

"xfce4-pm-helper --suspend"
  Mod4 + Shift + Delete

"/home/fabian/.screenlayout/home.sh"
	Mod4 + equal

"/home/fabian/.screenlayout/laptop.sh"
	Mod4 + minus

